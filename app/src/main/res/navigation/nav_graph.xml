<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.professionalhomework.ui.fragments.home.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/openHostoryFragment"
            app:destination="@id/historyFragment"
            app:enterAnim="@anim/enter_from_end"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_enter_anim"
            app:popExitAnim="@anim/exit_to_end" />
    </fragment>
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.example.professionalhomework.ui.fragments.history.HistoryFragment"
        android:label="HistoryFragment" />
</navigation>